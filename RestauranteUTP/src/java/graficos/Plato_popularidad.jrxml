	<queryString>
		<![CDATA[SELECT
     plato.`nombre_plato` AS plato_nombre_plato,
     COUNT(detalle_pedido.`cantidad_plato`) AS detalle_pedido_cantidad_plato
FROM
     `plato` plato INNER JOIN `detalle_pedido` detalle_pedido ON plato.`id_plato` = detalle_pedido.`id_plato`
GROUP BY nombre_plato
order by detalle_pedido_cantidad_plato DESC;]]>
	</queryString>
